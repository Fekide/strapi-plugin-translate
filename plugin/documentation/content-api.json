              },
              "sourceLocale": {
                "type": "string"
              },
              "targetLocale": {
                "type": "string"
              },
              "entityIds": {},
              "status": {
                "type": "string",
                "enum": [
                  "created",
                  "setup",
                  "running",
                  "paused",
                  "finished",
                  "cancelled",
                  "failed"
                ]
              },
              "failureReason": {},
              "progress": {
                "type": "number",
                "format": "float"
              },
              "autoPublish": {
                "type": "boolean"
              },
              "locale": {
                "type": "string"
              },
              "localizations": {
                "type": "array",
                "items": {
                  "oneOf": [
                    {
                      "type": "integer"
                    },
                    {
                      "type": "string"
                    }
                  ],
                  "example": "string or id"
                }
              }
            }
          }
        }
      },
